(()=>{(function(e,f){f.behaviors.challengeSelector={attach:function(v,c){var s=c.currentLanguage,g=c.defaultLanguage;if(s!==g){let a=function(t){e.each(t,function(n,i){e(this).val().startsWith(l)||e(this).val(l+e(this).val())})};var p=a;let l="";l="/"+s,a(e(".challenge-selector").find("[data-challenge-employer]").find("select").find("option")),a(e(".challenge-selector").find("[data-challenge-jobseeker]").find("select").find("option"))}e(".challenge-selector").find("[data-challenge-select]").on("change",function(){var l=e(this).val(),a=e(this).parents(".form-groups").first().find("[data-challenge-employer]"),t=a.find("select"),n=e(this).parents(".form-groups").first().find("[data-challenge-jobseeker]"),i=n.find("select"),r=e(this).parents(".challenge-selector").first().find("[data-challenge-redirect]");if(l==="an-employer"){if(a.hasClass("hidden")&&a.removeClass("hidden"),t.length&&r.length){var h=t.val();e(r).attr("href",h)}n.addClass("hidden")}else if(l==="a-job-seeker"){if(n.hasClass("hidden")&&n.removeClass("hidden"),i.length&&r.length){var h=i.val();e(r).attr("href",h)}a.addClass("hidden")}});function o(){var l=e(this).val(),a=e(this).parents(".challenge-selector").first().find("[data-challenge-redirect]");l.length&&a.length&&e(a).attr("href",l)}e(".challenge-selector").find("[data-challenge-employer]").find("select").on("change",o),e(".challenge-selector").find("[data-challenge-jobseeker]").find("select").on("change",o);var d=e("[data-challenge-redirect]");d.length&&e.each(d,function(l,a){var t=e(a).parent().find("[data-challenge-employer]").find("select");if(t.length){var n=t.val();n.length&&e(a).attr("href",n)}})}}})(jQuery,Drupal);})();
