(()=>{(function(e,a){a.behaviors.servicesOverview={attach:function(s,t){e('select[name="services_filter"]').change(function(){var i=e(this).val();e("li.bluex-services-overview-item").hide(),e('li.bluex-services-overview-item[data-service-filter-type="'+i+'"]').show()});var r=t.currentLanguage,v=t.defaultLanguage;if(r!==v){let i="";i="/"+r,e.each(e("li.bluex-services-overview-item").find("a"),function(c,n){e(this).attr("href").startsWith(i)||e(this).attr("href",i+e(this).attr("href"))})}}}})(jQuery,Drupal);})();
